    "postversion": "node ../publish-flat/dist/cli-copy.js -o flattened/package.json version",
    "start": "ts-node src/cli.ts",
    "test": "ts-node -P tsconfig.jasmine.json ../../node_modules/.bin/jasmine"
  },
  "version": "1.2.0"
