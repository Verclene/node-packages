{
  "author": "Vercelne <xdev58bo@gmail.com>",
  "bin": {
    "ntpclient": "dist/cli.js"
  },
  "dependencies": {
    "chrome-dgram": "3.0.6",
    "commander": "10.0.0"
  },
  "description": "A TypeScript and web implementation of the NTP Client Protocol",
  "devDependencies": {
    "rimraf": "4.1.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "ntpclient-web",
  "publishConfig": {
    "directory": "flattened"
  },
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/ntpclient",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
